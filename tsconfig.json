{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "baseUrl": "./src",
    "paths": {
      "baseUrl": ["./src/*"]
    },
    "outDir": "build",
  },
  "exclude": ["node_modules", "Build"]
}
